{
    "name": "@forchange/wind-sdk",
    "version": "0.0.42",
    "description": "风声web SDK",
    "main": "dist/index.js",
    "module": "dist/index-esm-min.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "license": "ISC",
    "author": "chenjiajing",
    "private": false,
    "homepage": "https://git.forchange.cn/web/sdk/wind-sdk",
    "repository": {
        "type": "git",
        "url": "https://git.forchange.cn/web/sdk/wind-sdk.git"
    },
    "publishConfig": {
        "registry": "https://nexus.forchange.cn/repository/npm-private/"
    },
    "keywords": [
        "@forchange",
        "@forchange/wind-sdk",
        "JavaScript"
    ],
    "scripts": {
        "dev": "rollup -c --watch",
        "build": "rm -rf dist/*  &&  NODE_ENV=production rollup  -c",
        "build:win": "rm -rf dist/*  && cross-env NODE_ENV=production rollup  -c",
        "reload": "livereload 'src/'",
        "ws": "node ./mock/ws.js",
        "lint": "eslint 'src/**/*.{ts}' --fix -o eslint_log.log",
        "test": "parcel test/index.html --out-dir test/dist",
        "watch": "npm-run-all --parallel reload dev test",
        "gc": "git-cz"
    },
    "dependencies": {
        "axios": "^0.19.2"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/plugin-transform-runtime": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "@commitlint/config-conventional": "^8.3.4",
        "@rollup/plugin-alias": "^3.0.1",
        "@rollup/plugin-commonjs": "^11.0.2",
        "@rollup/plugin-json": "^4.0.2",
        "@rollup/plugin-node-resolve": "^7.1.1",
        "@types/node": "^13.11.0",
        "commitizen": "^4.0.3",
        "core-js": "^3.6.4",
        "cross-env": "^7.0.2",
        "eslint": "^6.8.0",
        "livereload": "^0.9.1",
        "npm-run-all": "^4.1.5",
        "parcel": "^1.12.4",
        "prettier": "^2.0.2",
        "rollup": "^2.3.2",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-eslint": "^7.0.0",
        "rollup-plugin-sourcemaps": "^0.5.0",
        "rollup-plugin-terser": "^5.3.0",
        "rollup-plugin-typescript2": "^0.27.0",
        "rollup-watch": "^4.3.1",
        "tslib": "^1.11.1",
        "typescript": "^3.8.3",
        "ws": "^7.2.3"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-conventional-changelog"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "sh ./blockfieldlint.sh  && lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx,jsx,css,less,md}": [
            "prettier --write",
            "git add"
        ]
    }
}